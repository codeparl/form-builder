{
  "name": "form-builder",
  "version": "8.2.0",
  "type": "module",
  "description": "A Form Builder Component for Vue 3",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.es.js"
    },
    "./FormBuilder.css": "./dist/FormBuilder.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && cpx \"src/css/FormBuilder.css\" dist/FormBuilder.css",
    "ts": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "test": "vitest"
  },
  "keywords": [
    "vue3",
    "form-builder",
    "component-library"
  ],
  "repository": "https://github.com/codeparl/form-builder",
  "author": "Hassan Mugabo",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/node": "^24.3.1",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.28.0",
    "naive-ui": "^2.43.1",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.9.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vfonts": "^0.0.3",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.4",
    "vue-tsc": "^3.0.0"
  },
  "dependencies": {
    "@egoist/tailwindcss-icons": "^1.9.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "interactjs": "^1.10.27",
    "lucide-vue-next": "^0.544.0",
    "sortablejs": "^1.15.6",
    "vue-draggable-next": "^2.3.0",
    "vue-material-design-icons": "^5.3.1",
    "vuedraggable": "^4.1.0"
  }
}
